<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import org.controlsfx.control.PropertySheet?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="no.kantega.openai.fx.OpenaiChatModelViewController">
    <PropertySheet fx:id="openaiChatModelPropertySheet" searchBoxVisible="false" modeSwitcherVisible="false"/>
    <HBox>
        <Button fx:id="streamingChatModelAction" text="Create custom streaming chat model" onAction="#createStreamingChatModel"/>
    </HBox>
</VBox>
