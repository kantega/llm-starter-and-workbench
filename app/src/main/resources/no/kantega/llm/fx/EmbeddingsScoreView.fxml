<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="no.kantega.llm.fx.EmbeddingsScoreViewController">
    <TextArea fx:id="embeddingsText1" prefRowCount="5"/>
    <TextArea fx:id="embeddingsText2" prefRowCount="20"/>
    <Button fx:id="embeddingsScoreAction" text="Score with %s" onAction="#handleEmbeddingsScore"/>
    <ListView fx:id="matchesListView" VBox.vgrow="ALWAYS"/>
</VBox>
