<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import org.controlsfx.control.PropertySheet?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="no.kantega.ollama.fx.OllamaChatModelViewController">
    <PropertySheet fx:id="ollamaChatModelPropertySheet"/>
    <HBox>
        <Button fx:id="chatModelAction" text="Chat model" onAction="#createAndUpdateChatModel"/>
        <Button fx:id="streamingChatModelAction" text="Streaming chat model" onAction="#createAndUpdateStreamingChatModel"/>
    </HBox>
</VBox>
